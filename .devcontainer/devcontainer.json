{
    "name": "MiniTwitter Django Dev",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "web",
    "workspaceFolder": "/app",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.vscode-pylance",
          "ms-azuretools.vscode-docker",
          "batisteo.vscode-django"
        ]
      }
    },
    "forwardPorts": [8000],
    "postCreateCommand": "pip install -r requirements.txt"
  }
  